{{#if productos}}
<table class="table table-striped-columns">
    <thead>
    <tr>
        <th>Nombre</th>
        <th>Precio</th>
        <th>URL</th>
    </tr>
    </thead>
    <tbody>
        {{#each productos}}
            <tr>
                <td>{{this.producto}}</td>
                <td>{{this.precio}}</td>
                <td><img src="{{this.url}}" alt="{{this.producto}}"> </td>
            </tr>
        {{/each}}
    </tbody>
</table>
{{else}}
    <h2>No hay productos</h2>
{{/if}}
<a href="/"><button class="btn btn-primary">Volver</button></a>
